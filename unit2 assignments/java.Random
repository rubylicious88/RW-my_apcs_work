import java.Random;

public class CoinFlip {
    public static String flipCoin() {
        Random rand = new Random();
        return rand.nextBoolean() ? "Heads" : "Tails";
    }

    public static void main(String[] args) {
        for (int i = 0; i < 20; i++) {
            System.out.println(flipCoin());
        }
    }
}


